{
  "name": "desa-api",
  "version": "1.0.50",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "bun run --watch src/index.ts",
    "lint": "bunx biome lint --write",
    "format": "bunx biome format --write",
    "check": "bun run typecheck && bun run lint",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio --port 5555",
    "db:seed": "bun run src/db/seed/index.ts"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.4.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.5",
    "elysia": "latest",
    "jose": "^6.1.0",
    "pg": "^8.16.3",
    "pino": "^9.12.0",
    "postgres": "^3.4.7"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@types/pg": "^8.15.5",
    "bun-types": "latest",
    "drizzle-kit": "^0.31.5",
    "pino-pretty": "^13.1.1",
    "tsx": "^4.20.6"
  },
  "module": "src/index.js"
}
